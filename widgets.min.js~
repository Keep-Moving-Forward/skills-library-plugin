                    if (!e(this).is("select")) {
                        e.error("Sorry, this plugin only works with select fields.")
                    }
                    if (i.hasOwnProperty(n)) {
                        i.init(r, this);
                        return i[n]()
                    } else if (typeof n === "object" || !n) {
                        r = n;
                        i.init(r, this);
                        return i.show()
                    } else {
                        e.error("Method " + n + " does not exist on jQuery.barrating")
                    }
                })
            };
