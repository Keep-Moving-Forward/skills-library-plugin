                                    u.addClass("br-selected")[o]().addClass("br-selected");
                                    a = u.attr("data-rating-value");
                                    f = u.attr("data-rating-text");
                                    if (u.hasClass("br-current") && t.data("barrating").deselectable) {
                                        u.removeClass("br-selected br-current")[o]().removeClass("br-selected br-current");
                                        a = "", f = "";
                                        if (!e.isEmptyObject(u.parent().prev("label")))
                                            u.parent().prev("label").show()
                                    } else {
                                        i.removeClass("br-current");
                                        u.addClass("br-current");
                                        if (!e.isEmptyObject(u.parent().prev("label")))
                                            u.parent().prev("label").hide()
                                    }
